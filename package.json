{
  "name": "nodejs-seed",
  "version": "1.0.0",
  "description": "Nodejs + TypeScript + Swagger + Docker + Express seed project",
  "main": "index.js",
  "scripts": {
    "chk-updates": "npm-check --skip-unused",
    "preinstall": "npm install -g swagger tslint@next typescript@next nodemon",
    "start": "npm run build & node ./dist/index.js",
    "dev": "npm run watch & nodemon ./dist/index.js",
    "debug": "npm run watch & node-debug ./index.js",
    "clean": "rimraf dist",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "tslint ./src/**/*.ts",
    "test": "mocha ./dist/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/nodejs-seed.git"
  },
  "keywords": [
    "typescript",
    "swagger",
    "docker",
    "express"
  ],
  "engines": {
    "node": "~6.2"
  },
  "author": "Swimlane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/nodejs-seed/issues"
  },
  "homepage": "https://github.com/swimlane/nodejs-seed#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "config": "^1.21.0",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "express-openapi-coercion": "^0.4.1",
    "express-openapi-defaults": "^0.4.2",
    "express-openapi-validation": "^0.7.0",
    "js-yaml": "^3.6.1",
    "reflect-metadata": "^0.1.3",
    "require-all": "^2.0.0",
    "routing-controllers": "^0.6.0-alpha.9",
    "swagger-client": "^2.1.15",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.23-alpha",
    "@types/config": "0.0.23-alpha",
    "@types/cors": "0.0.23-alpha",
    "@types/express": "^4.0.23-alpha",
    "@types/js-yaml": "^3.5.21-alpha",
    "@types/node": "^4.0.22-alpha",
    "@types/serve-static": "^1.7.21-alpha",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "supertest": "^1.2.0",
    "tslint": "^3.12.0-dev.2",
    "typescript": "^2.0.0-dev.20160629"
  }
}
